{
  "name": "pennerooney_cookbook_api",
  "version": "1.0.0",
  "description": "api for the pennerooney_cookbook",
  "main": "src/index.ts",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "testdb": "dotenv -e .env.test -- npx prisma db push",
    "migrate:mysql": "dotenv -e .env.development -- npx prisma migrate dev",
    "dev": "dotenv -e .env.development -- nodemon src/index.ts",
    "test": "dotenv -e .env.test -- jest --watchAll --runInBand"
  },
  "author": "crazyapple48",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "joi": "^17.13.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "esbuild-register": "^3.6.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.2"
  }
}
