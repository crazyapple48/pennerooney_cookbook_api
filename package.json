{
  "name": "pennerooney_cookbook_api",
  "version": "1.0.0",
  "description": "api for the pennerooney_cookbook",
  "main": "build/index.js",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node ./build/api/index.js\"",
    "start": "run build && node .",
    "build": "tsc",
    "db:up": "echo creating && npm run db:create && echo created && npm run db:migrate && echo migrated",
    "db:reset": "npm run db:drop && npm run db:up",
    "db:create": "sequelize db:create",
    "db:drop": "[[ $NODE_ENV == production ]] && exit 1 || sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:rollback:all": "sequelize db:migrate:undo:all",
    "test": "test"
  },
  "author": "crazyapple48",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "joi": "^17.13.3",
    "mysql2": "^3.11.2",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@types/sequelize": "^4.28.20",
    "@types/validator": "^13.12.1",
    "morgan": "^1.10.0",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.2"
  }
}
